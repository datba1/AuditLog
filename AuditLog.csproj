<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
		<PackageId>datba.AuditLog</PackageId>
		<Version>1.0.0</Version>
		<Authors>datba</Authors>
		<Company>datba</Company>
		<Description>Audit log library for .NET</Description>
		<PackageTags>audit log logging</PackageTags>
		<RepositoryUrl>https://github.com/datba1/AuditLog</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.1" />
  </ItemGroup>

</Project>
